@model IEnumerable<clinica2.Models.site>

@{
    ViewData["Title"] = "Index";
}

<h1 style="color: #a35f56;">Участки</h1>

<table class="table">
    <thead>
        <tr>
            <th style="color: #a35f56;">Номер участка</th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.site_number)
            </td>
        </tr>
}
    </tbody>
</table>
